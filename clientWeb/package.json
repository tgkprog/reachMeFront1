{
  "name": "reachme-web",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "predev": "node scripts/generate-config.js",
    "dev": "vite",
    "dev:8087": "node scripts/generate-config.js && vite --port 8087",
    "dev:8088": "node scripts/generate-config.js && vite --port 8088",
    "prebuild": "node scripts/generate-config.js",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "vue": "3.5.24",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "dotenv": "^17.2.3",
    "typescript": "^5.3.3",
    "vite": "^7.2.4",
    "vue-tsc": "^1.8.27"
  }
}
