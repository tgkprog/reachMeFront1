# Development Environment
# Vite env vars must be prefixed with VITE_

# Base URL of backend dev server
VITE_API_BASE_URL=https://b.c.sel2in.com/n1

# Google OAuth Web Client ID
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Password login configuration
VITE_PASSWORD_LOGIN_ENABLED=true
VITE_PASSWORD_LOGIN_ENDPOINT=/user/login

# Polling configuration
VITE_DEFAULT_POLL_INTERVAL=60
VITE_MIN_POLL_INTERVAL=10
VITE_MAX_POLL_INTERVAL=180

# Feature flags
VITE_ENABLE_WEB_NOTIFICATIONS=true
VITE_DEBUG_MODE=false

# Web app title
VITE_APP_TITLE=ReachMe (Dev)
